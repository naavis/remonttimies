<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <GLEWBaseDir>$(SOLUTIONDIR)../Libraries/glew-1.12.0-win32/glew-1.12.0/</GLEWBaseDir>
  </PropertyGroup>
  <PropertyGroup>
    <ExecutablePath>$(GLEWBaseDir)bin/Release/Win32/;$(ExecutablePath)</ExecutablePath>
    <IncludePath>$(GLEWBaseDir)include;$(IncludePath)</IncludePath>
    <LibraryPath>$(GLEWBaseDir)lib/Release/Win32/;$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>glew32.lib;opengl32.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="GLEWBaseDir">
      <Value>$(GLEWBaseDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>